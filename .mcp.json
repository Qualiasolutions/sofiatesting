{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "postman": {
      "command": "npx",
      "args": [
        "-y",
        "@anthropic-ai/mcp-server-postman@latest"
      ],
      "env": {
        "POSTMAN_API_KEY": ""
      }
    }
  }
}